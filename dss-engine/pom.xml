		<dependency>
			<groupId>org.streamconnect.dss.common</groupId>
			<artifactId>dss-common</artifactId>
			<version>${project.parent.version}</version>
		</dependency>
		<dependency>
			<groupId>org.streamconnect.dss.metadata.connection</groupId>
